---
slug: <%= modelNamePlural %>/show
---
{% liquid

  assign <%= modelName %>_id = context.params.slug3 | split: '-' | last
  function <%= modelName %> = '<%= options.outputLogicFolder %>/queries/<%= modelNamePlural %>/find', id: <%= modelName %>_id
  include '<%= options.outputLogicFolder %>/present_or_404', property: <%= modelName %>.id

  include '<%= options.outputThemeFolder %>/simple/<%= modelNamePlural %>/show', <%= modelName %>: <%= modelName %>
%}
