---
slug: <%= modelNamePlural %>
method: put
---
{% liquid
  include '<%= config.logicDependencyFolder %>/logged_in_or_redirect'
  function object = '<%= config.logicDependencyFolder %>/commands/call', object: context.params.<%= modelName %>, command: '<%= modelNamePlural %>/update', command_path: '<%= config.outputLogicFolder %>', selection: 'record_update'
  if object.valid
    include 'modules/func/redirect_to', url: '/<%= modelNamePlural %>'
  else
    include '<%= config.outputThemeFolder %>/simple/<%= modelNamePlural %>/edit', object: object
  endif
%}
