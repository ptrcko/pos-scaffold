{% liquid
  function object = '<%= options.outputLogicFolder %>/commands/<%= modelNamePlural %>/delete/check',  object: object

  if object.valid
    function object = '<%= options.outputLogicFolder %>/commands/execute', mutation_name: '<%= options.outputLogicFolder %>/<%= modelNamePlural %>/delete', selection: 'record_delete', object: object
  endif

  return object
%}
