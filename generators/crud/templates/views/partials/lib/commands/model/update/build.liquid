{% parse_json object %}
  {
    "id": {{ object.id | json }},
<% attributes.forEach((attr, i) => { -%>
    "<%= attr.name %>": {{ object.<%= attr.name %><%= liquidTagMap[attr.type] %> | json }}<% if (i+1 < attributes.length){ %>,<% } %>
<% }); -%>
  }
{% endparse_json %}

{% liquid
  return object
%}
