---
slug: <%= modelNamePlural %>/show
---
{% liquid

  assign <%= modelName %>_id = context.params.slug3 | split: '-' | last
  function <%= modelName %> = 'queries/<%= modelNamePlural %>/find', id: <%= modelName %>_id
  include 'present_or_404', property: <%= modelName %>.id

  include '<%= outputThemeFolder %>/<%= outputTheme %>/<%= modelNamePlural %>/show', <%= modelName %>: <%= modelName %>
%}
