---
slug: <%= modelNamePlural %>
method: delete
---
{% liquid
  function current_profile = 'current_profile', user_id: context.current_user.id
  include 'profile_present_or_not_authenticated', profile: current_profile

  function object = 'queries/<%= modelNamePlural %>/find', id: context.params.id
  function object = 'commands/<%= modelNamePlural %>/delete', object: object

  render 'theme/json/shared/show', object: object
%}
